[user]
    name = Martin Krizek
    email = mkrizek@redhat.com
[color]
    branch = auto
    diff = auto
    status = auto
    ui = auto
[alias]
    co = checkout
    br = branch -v
    ci = commit
    st = status
    last = log -1 HEAD
    pod = push origin develop
    pom = push origin master
    chp = cherry-pick
    cia = commit -a
    lg = log --decorate=full
[color "diff"]
    whitespace = red reverse
[push]
	default = simple
